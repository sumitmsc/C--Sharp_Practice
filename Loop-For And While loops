using System;

namespace Loops
{
    class Program
    {
        static void Main(string[] args)
        {
            // For loop
            for (int i = 1; i <= 5; i++)
            {
                Console.WriteLine("Iteration " + i);
            }

            // While loop
            int count = 0;
            while (count < 3)
            {
                Console.WriteLine("Count: " + count);
                count++;
            }
        }
    }
}
